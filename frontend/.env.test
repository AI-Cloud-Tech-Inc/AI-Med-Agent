# Test Environment Configuration
NODE_ENV=test
NEXT_PUBLIC_API_URL=http://localhost:3001/api
NEXT_PUBLIC_APP_NAME=AI-Med-Agent
NEXT_PUBLIC_APP_ENV=test

# Database (Test)
DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/ai_med_agent_test
REDIS_URL=redis://localhost:6379/1

# AWS Configuration (Test)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test-access-key
AWS_SECRET_ACCESS_KEY=test-secret-key
AWS_ENDPOINT=http://localstack:4566

# Authentication (Test)
JWT_SECRET=test-jwt-secret-key-for-testing-only
SESSION_SECRET=test-session-secret-key

# Feature Flags
ENABLE_AUTONOMOUS_DECISIONS=false
ENABLE_COST_OPTIMIZATION=false
ENABLE_ANALYTICS=true

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Test Timeouts
TEST_TIMEOUT=30000
E2E_TIMEOUT=60000

# Mock Services
MOCK_AWS_ORGANIZATIONS=true
MOCK_EXTERNAL_APIS=true
